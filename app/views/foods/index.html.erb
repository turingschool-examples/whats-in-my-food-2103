  <h3>Results for <%= params[:q] %></h3>
  <h4>Total Results: <%= @total_results %></h4>
  <div id='results'>
  <% @search_results.each do |food| %>
    <div class="single-result">
    <p>GTIN/UPC: <%= food.code %></p>
    <p>Description: <%= food.description %></p>
    <p>Brand Owner: <%= food.brand_owner %></p>
    <p>Ingredients: <%= food.ingredients %></p>
    <br>
    <br>
  </div>
  <% end %>
  </div>
