<h1>Your query found <%= @foods.total_hits %> results for <%= @q %></h1>

<% @foods.foods(10).each do |food| %>
<div class="food">
  <h4>Brand Owner: <%= food.brand_owner %></h4>
  <ul>
    <li>GTIN/UPC: <%= food.gtin_upc %></li>
    <li>Description: <%= food.description %></li>
    <li>Ingredients: <%= food.ingredients %></li>
  </ul>
</div>
<% end %>